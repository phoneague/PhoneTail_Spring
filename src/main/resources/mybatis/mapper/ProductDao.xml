<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.himedia.phonetail_spring.dao.IProductDao">
    <select id="getBrandList" resultType="com.himedia.phonetail_spring.dto.ProductVO">
        select * from product
    </select>

    <select id="getNewList" resultType="com.himedia.phonetail_spring.dto.ProductVO">
        select * from new_product
    </select>

    <select id="getProduct" resultType="com.himedia.phonetail_spring.dto.ProductVO">
        select * from product where pseq=#{param1}
    </select>

</mapper>